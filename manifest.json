{
    "manifest_version": 3,
    "name": "Random Tab Refresher",
    "version": "1.0",
    "description": "Refreshes the current tab at random intervals between 10-15 minutes when activated",
    "permissions": ["activeTab", "alarms", "tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon.png"
    }
  }